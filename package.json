  "author": "Rafael Neri <rafael.neri@gmail.com> (https://github.com/rafaneri)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafaneri/pr9200-rfid/issues"
  },
  "homepage": "https://github.com/rafaneri/pr9200-rfid#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.1.0",
    "typescript": "^2.9.1",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "@types/q": "^1.5.0",
    "@types/serialport": "^8.0.1",
    "phychips-rcp": "^1.1.16",
    "q": "^1.5.1",
    "serialport": "^9.0.3"
  }
}
